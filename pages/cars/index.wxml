<view class="container">
  <view class="icon-container">
    <image src="/images/icons/navBar/home-light.png" class="custom-icon"></image>
    <image src="/images/icons/navBar/filter-light.png" class="custom-icon"></image>
    <image src="/images/icons/navBar/bell-light.png" class="custom-icon"></image>
  </view>

  <text class="h2">Available cars in {{city}}</text>
  <view class="card-grid">
    <view class="card" wx:for="{{cars}}">
      <image src="{{item.car_image}}" class="card-image" mode="aspectFill" wx:key="key"/>
      <view class="car-info">
      <text class="p">{{item.car_model}}</text>
      <view class="p light">5.0</view>
      </view>
    </view>
  </view>
</view>