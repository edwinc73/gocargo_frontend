.toggle{
  width: 120rpx;
  height: 60rpx;
  display: block;
  background-color: orange;
  color:#1d1d1d;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 16rpx;
}

.title{
  margin-top: 10px;
  margin-bottom: 24px;
}

.status{
  font-size: 16px;
  border-style: solid;
  border-width: thin;
  border-radius: 30px;
  border-color: #FFA02A;
  padding: 10rpx 20rpx;
  text-align: center;
  align-items: center;
}

.booking-card{
  margin: 40rpx auto;
  border-radius: 35px;
  background-color: rgba(217,217,217,0.35);
  padding: 15px 20px;
}

.booking-card .flex{
  align-items: center;
}

.completed{
  text-align: left;
}

.dash{
  height: 1px;
  width: 20px;
  display: block;
  background-color: #CACACA;
}

.date-container{
  width: 70%;
  justify-content: left;
  gap: 10px;
}

.booking-card{
  display: flex;
  flex-direction: column;
  gap: 5px;
  margin-top: 0;
}